{
  "name": "canto-web-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "buildProd": "ng build --configuration=production",
    "clearS3": "aws s3 rm s3://piedefi.com --recursive",
    "uploadToS3": "aws s3 sync ./dist/canto-web-ui/browser s3://piedefi.com",
    "cfInvalidate": "aws cloudfront create-invalidation --distribution-id E29G27AAJ89404 --paths '/'",
    "deploy": "yarn buildProd && yarn clearS3 && yarn uploadToS3 && yarn cfInvalidate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@forevolve/bootstrap-dark": "^4.0.2",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0-rc.2",
    "@ng-icons/bootstrap-icons": "^27.3.1",
    "@ng-icons/core": "^27.3.1",
    "@ng-icons/cryptocurrency-icons": "^27.3.1",
    "@ng-icons/iconoir": "^27.3.1",
    "@ng-icons/jam-icons": "^27.3.1",
    "@ng-icons/octicons": "^27.3.1",
    "@ng-icons/tdesign-icons": "^27.3.1",
    "@popperjs/core": "^2.11.8",
    "apexcharts": "^3.48.0",
    "bootstrap": "^5.3.3",
    "ng-animate": "^2.0.1",
    "ng-apexcharts": "^1.10.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.4",
    "@angular/cli": "^17.3.4",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/localize": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.2"
  }
}
